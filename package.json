{
  "name": "homebrewery",
  "description": "Create authentic looking D&D homebrews using only markdown",
  "version": "3.0.8",
  "engines": {
    "node": "16.11.x"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/naturalcrit/homebrewery.git"
  },
  "scripts": {
    "dev": "node scripts/dev.js",
    "quick": "node scripts/quick.js",
    "build": "node scripts/buildHomebrew.js",
    "buildall": "node scripts/buildHomebrew.js && node scripts/buildAdmin.js",
    "lint": "eslint --fix **/*.{js,jsx}",
    "lint:dry": "eslint **/*.{js,jsx}",
    "circleci": "npm test && eslint **/*.{js,jsx} --max-warnings=0",
    "verify": "npm run lint && npm test",
    "test": "jest",
    "test:dev": "jest --verbose --watch",
    "test:basic": "jest tests/markdown/basic.test.js --verbose",
    "test:mustache-span": "jest tests/markdown/mustache-span.test.js --verbose",
    "test:route": "jest tests/routes/static-pages.test.js --verbose",
    "phb": "node scripts/phb.js",
    "prod": "set NODE_ENV=production && npm run build",
    "postinstall": "npm run buildall",
    "start": "node server.js"
  },
  "author": "stolksdorf",
  "license": "MIT",
  "eslintIgnore": [
    "build/*"
  ],
  "jest": {
    "testTimeout" : 15000,
    "modulePaths": [
      "mode_modules",
      "shared",
      "server"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.17.10",
    "@babel/plugin-transform-runtime": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.16.7",
    "body-parser": "^1.20.0",
    "classnames": "^2.3.1",
    "codemirror": "^5.65.3",
    "cookie-parser": "^1.4.6",
    "create-react-class": "^15.7.0",
    "dedent-tabs": "^0.10.1",
    "express": "^4.18.1",
    "express-async-handler": "^1.2.0",
    "express-static-gzip": "2.1.6",
    "fs-extra": "10.1.0",
    "googleapis": "100.0.0",
    "js-yaml": "^4.1.0",
    "jwt-simple": "^0.5.6",
    "less": "^3.13.1",
    "lodash": "^4.17.21",
    "marked": "4.0.14",
    "marked-extended-tables": "^1.0.3",
    "markedLegacy": "npm:marked@^0.3.19",
    "moment": "^2.29.3",
    "mongoose": "^6.3.1",
    "nanoid": "3.3.3",
    "nconf": "^0.12.0",
    "query-string": "7.1.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-frame-component": "4.1.3",
    "react-router-dom": "5.3.0",
    "sanitize-filename": "1.6.3",
    "superagent": "^6.1.0",
    "vitreum": "git+https://git@github.com/calculuschild/vitreum.git"
  },
  "devDependencies": {
    "eslint": "^8.14.0",
    "eslint-plugin-react": "^7.29.4",
    "jest": "^28.0.3",
    "supertest": "^6.2.3"
  }
}
